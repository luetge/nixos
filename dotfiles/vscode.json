{
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascript]": {
    "editor.defaultFormatter": "vscode.typescript-language-features"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[python]": {
    "editor.formatOnType": true
  },
  "autoDocstring.docstringFormat": "sphinx",
  "editor.accessibilitySupport": "off",
  "editor.fontFamily": "'FiraCode Nerd Font Mono', Consolas, 'Courier New', monospace",
  "editor.fontLigatures": true,
  "editor.fontSize": 16,
  "editor.formatOnSave": true,
  "editor.minimap.enabled": false,
  "editor.multiCursorModifier": "ctrlCmd",
  "editor.renderWhitespace": "none",
  "editor.suggestSelection": "first",
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "files.autoSave": "onFocusChange",
  "git.autofetch": true,
  "git.confirmSync": false,
  "git.ignoreMissingGitWarning": true,
  "gitlens.gitCommands.skipConfirmations": [
    "fetch:command",
    "stash-push:command",
    "switch:command",
    "push:command"
  ],
  "jupyter.alwaysTrustNotebooks": true,
  "jupyter.askForKernelRestart": false,
  "jupyter.debugJustMyCode": false,
  "jupyter.experimental.debugging": true,
  "jupyter.jupyterServerType": "local",
  "nix.enableLanguageServer": true,
  "nixEnvSelector.args": "",
  "notebook.cellToolbarLocation": {
    "default": "right",
    "jupyter-notebook": "left"
  },
  "python.formatting.autopep8Args": ["--max-line-length=88"],
  "python.formatting.provider": "black",
  "python.languageServer": "Pylance",
  "python.linting.flake8Args": ["--max-line-length=88", "--extend-ignore=E203"],
  "python.linting.flake8Enabled": true,
  "python.linting.mypyEnabled": true,
  "python.linting.pylintArgs": ["--enable=W0614"],
  "python.linting.pylintEnabled": false,
  "python.pythonPath": "/bin/python3",
  "python.terminal.launchArgs": ["-m", "IPython", "--no-autoindent"],
  "remote.SSH.defaultExtensions": [
    "ms-python.python",
    "arrterian.nix-env-selector",
    "aaron-bond.better-comments",
    "ryanluker.vscode-coverage-gutters",
    "eamodio.gitlens",
    "njpwerner.autodocstring",
    "visualstudioexptteam.vscodeintellicode",
    "usernamehw.errorlens",
    "jnoortheen.nix-ide",
    "oderwat.indent-rainbow",
    "ban.spellright",
    "bungcip.better-toml",
    "mkhl.direnv"
  ],
  "rust-analyzer.cargo.allFeatures": true,
  "rust-analyzer.server.path": "rust-analyzer",
  "tabnine.experimentalAutoImports": true,
  "terminal.integrated.windowsEnableConpty": false,
  "vscode-neovim.neovimExecutablePaths.darwin": "/usr/local/bin/nvim",
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "workbench.colorTheme": "Material Theme High Contrast",
  "workbench.editorAssociations": {
    "*.ipynb": "jupyter-notebook"
  },
  "workbench.startupEditor": "newUntitledFile",
  "nix.serverPath": "nil",
  "nix.formatterPath": "nixfmt"
}
